package ArrayPack1;

public class DigonalLeft {
	
	public static void main(String[] args) {
		
		
		int a[][]=new int[4][4];
		
		int m=20;
		
		for(int i=0;i<a.length;i++) {
			
			for(int j=0;j<a.length;j++);
			
			int n = ++m;
			
			if(i==j) {
				
				System.out.print([i][j]=n);
			}
			
			
			
		}
		
		
		
		
	}
	
	
	
	
	
	
	
	
	
	
	

}
