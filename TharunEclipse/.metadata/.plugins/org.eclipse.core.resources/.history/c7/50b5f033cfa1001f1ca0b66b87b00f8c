package ArrayPack1;

public class B {

	public static void main(String[] args) {

		int b[][] = new int[5][5];

		int k = 10;

		for (int i = 0; i < b.length; i++) {
			for (int j = 0; j < b.length; j++) {
                 for(int l=0;l < b.length;l++)
				int m = ++k;

				if (i == j) {
					System.out.print(b[i][j] = k);
				} else if (i + j == b.length - 1) {
					System.out.print(b[i][j] = k);
				}
				else {
					System.out.print(" # ");
					
				}
					
					
				}
System.out.println();
			}

		}

	}


