package ArrayPack1;

public class person {
	
	int age;
	String name;
	double height;
	double weight;

	public person(String s, double d1, double d2, int i) {
		this.age=i;
		this.name=s;
		this.height=d1;
		this.weight=d2;
	}
	
	
	
	

	@Override
	public String toString() {
		return "person [age=" + age + ", name=" + name + ", height=" + height + ", weight=" + weight + "]";
	}
	
