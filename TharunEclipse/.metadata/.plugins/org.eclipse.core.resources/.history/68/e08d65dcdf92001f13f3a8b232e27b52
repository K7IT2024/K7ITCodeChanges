package ObjectPackage1;

public class Calculator {
	
	int age;
	String name;
	double height;
	double weight;
	
	
	public Calculator(){
		
	}
	
	public Calculator(int age,String name,double height,double weight) {
		this.age= age;
		this.name= name;
		this.height=height;
		this.weight=weight;
	}
	

}
