package ArrayPack1;

public class Animal {
	
	
	
	public Animal(int i, String string, double d, double e) {
		this.age=i;
		this.name=string;
		this.height=d;
		this.weight=e;
	}

	public static void main(String[] args) {
		
		
		Animal a[]=new Animal[5];
		
	System.out.println(a);
	
	
	a[0]=new Animal(20,"ishu",9.0,90.00);
	
	
	
	System.out.println(a[0]);
	
	
	
		
		
	}
	
	
	
	
	
	

}
