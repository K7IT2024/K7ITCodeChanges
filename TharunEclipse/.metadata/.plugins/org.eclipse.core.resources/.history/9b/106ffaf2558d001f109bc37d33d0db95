package com.K7it.pack1;

public interface Vehical1 {
	
	void fixEngine();
    void fixBreaks();
    void fixBody();
    void fixTyres();
    
    
    public class Lorry implements Vehical1{

		
	public void fixEngine() {
		System.out.println("fixed Engine body");
		}
	}
			
    public void fixBreaks() {
		System.out.println("fixed Engine body");
		}
	
	
}