package ArrayPack1;

import java.util.Arrays;
import java.util.Comparator;

public class Sorting {

	public static void main(String[] args) {

		short[] a = new short[5];

		a[0] = 20;
		a[1] = 90;
		a[2] = 80;
		a[3] = 30;
		a[4] = 10;

		Arrays.sort(a,Comparator.reverseOrder());
		for (int v : a)
			System.out.println(v);
     //System.out.println(a.length);
	}
}
