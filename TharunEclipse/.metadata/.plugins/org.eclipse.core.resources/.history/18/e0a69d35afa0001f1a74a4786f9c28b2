package ArrayPack1;

public class Arrays1 {

	//static int i[];
	
	static int i [] = new int[8];
	
	@Override
	public String toString() {
		return "Arrays1 []";
	}

	public static void main(String[] args) {
		
		
		System.out.println(i);
		
		System.out.println(i[2]);
		
	}
	
	
	
	
	
	
	
	
	
	
	
	
}





