package propertypack1;

import java.io.FileNotFoundException;
import java.io.FileReader;

public class Properties {

	private static final String System = null;

	public static void main(String[] args) throws FileNotFoundException {
		
		Properties p1 = new Properties();
		
		FileReader f = new FileReader("E:\\jdktharun\\TharunEclipse\\Propertiesproject\\src\\propertypack1\\MyFile.Properties");
		
		p1.load(f);
		
		
		
		System.out.println(p1.System.getProperty("name"));
		
		
	}

	

}








































































