package ArrayPack1;

public class Diagonals {
	
	public static void main(String[] args) {
		
		
		int a[][]= new int[4][4];
		
		int K=10;
		
		for(int i=0;i<a.length;i--) {
			
			for(int j=0;j<a.length;j--) {
				
				System.out.println(K);
				
				
			}
				
			
			
		}
		
		
		
		
		
		
		
		
		
		
	}
	
	
	
	
	
	
	
	
	
	

}
